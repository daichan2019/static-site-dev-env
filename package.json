{
  "name": "static-site-dev-env",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "run-p clean dev:*",
    "dev:template": "eleventy --serve",
    "dev:assets": "webpack --mode development --watch",
    "build": "run-p clean build:*",
    "build:template": "npx @11ty/eleventy",
    "build:assets": "webpack --mode production",
    "lint": "run-p -l -c lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src/js/**/*.js",
    "fix": "run-s fix:*",
    "fix:prettier": "yarn lint:prettier -- --write",
    "fix:eslint": "yarn lint:eslint -- --fix"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.0.4",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "fibers": "^5.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob-loader": "^1.1.0",
    "mini-css-extract-plugin": "^1.3.2",
    "postcss": "^8.2.10",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.1.0",
    "postcss-sort-media-queries": "^2.2.14",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "yarn-run-all": "^3.1.1"
  },
  "browserslist": [
    "last 1 version",
    "> 3% in JP",
    "ie >= 11",
    "iOS >= 12",
    "Android >= 5.1"
  ],
  "dependencies": {
    "element-closest": "^3.0.2",
    "es6-promise": "^4.2.8",
    "focus-visible": "^5.2.0",
    "micromodal": "^0.4.6",
    "object-fit-images": "^3.2.4",
    "picturefill": "^3.0.3",
    "svg4everybody": "^2.1.9"
  }
}
